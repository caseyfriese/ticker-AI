                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Graphs</h3>
                        </div>
                        <div class="panel-body">
                            Graphs Dashboard
                        </div>
                    </div>
                </div>
                <br />
                <div class="col-sm-12 col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Symbols</h3>
                        </div>
                        <div class="panel-body">
                            Symbols Dashboard
                        </div>
                    </div>
                </div>
            </div>
